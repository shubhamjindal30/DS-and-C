#include<iostream>
using namespace std;
int main()
{
    int a[20],b[20];
    cout<<"Enter size of both arrays"<<endl;
    int s1,s2;
    cin>>s1>>s2;
    
    int i,j;
    cout<<"Enter first array"<<endl;
    for(i=0;i<s1;i++)
    {
        cin>>a[i];
    }
    cout<<"Enter second array"<<endl;
    for(i=0;i<s2;i++)
    {
        cin>>b[i];
    }
    
    cout<<endl<<"Common Elements are: "<<endl;
    for(i=0;i<s1;i++)
    {
        for(j=0;j<s2;j++)
        {
            if(a[i]==b[j])
            {
                cout<<a[i]<<" ";
            }
        }
    }
}
